            ></el-input>\n            <el-input\n              v-if=\"contents.inputIcon == 0\"\n              v-model=\"searchForm.yonghuming\"\n              placeholder=\"用户名\"\n              clearable\n            ></el-input>\n          </el-form-item>\n          <el-form-item :label=\"contents.inputTitle == 1 ? '姓名' : ''\">\n            <el-input\n              v-if=\"contents.inputIcon == 1 && contents.inputIconPosition == 1\"\n              prefix-icon=\"el-icon-search\"\n              v-model=\"searchForm.xingming\"\n              placeholder=\"姓名\"\n              clearable\n            ></el-input>\n            <el-input\n              v-if=\"contents.inputIcon == 1 && contents.inputIconPosition == 2\"\n              suffix-icon=\"el-icon-search\"\n              v-model=\"searchForm.xingming\"\n              placeholder=\"姓名\"\n              clearable\n            ></el-input>\n            <el-input\n              v-if=\"contents.inputIcon == 0\"\n              v-model=\"searchForm.xingming\"\n              placeholder=\"姓名\"\n              clearable\n            ></el-input>\n          </el-form-item>\n          <el-form-item>\n            <el-button\n              v-if=\"\n                contents.searchBtnIcon == 1 &&\n                contents.searchBtnIconPosition == 1\n              \"\n              icon=\"el-icon-search\"\n              type=\"success\"\n              @click=\"search()\"\n              >{{ contents.searchBtnFont == 1 ? \"查询\" : \"\" }}</el-button\n            >\n            <el-button\n              v-if=\"\n                contents.searchBtnIcon == 1 &&\n                contents.searchBtnIconPosition == 2\n              \"\n              type=\"success\"\n              @click=\"search()\"\n              >{{ contents.searchBtnFont == 1 ? \"查询\" : \"\"\n              }}<i class=\"el-icon-search el-icon--right\"\n            /></el-button>\n            <el-button\n              v-if=\"contents.searchBtnIcon == 0\"\n              type=\"success\"\n              @click=\"search()\"\n              >{{ contents.searchBtnFont == 1 ? \"查询\" : \"\" }}</el-button\n            >\n          </el-form-item>\n        </el-row>\n\n        \n      </el-form>\n      <div class=\"table-content\">\n        <el-table\n          class=\"tables\"\n          :size=\"contents.tableSize\"\n          :show-header=\"contents.tableShowHeader\"\n          :header-row-style=\"headerRowStyle\"\n          :header-cell-style=\"headerCellStyle\"\n          :border=\"contents.tableBorder\"\n          :fit=\"contents.tableFit\"\n          :stripe=\"contents.tableStripe\"\n          :style=\"{\n            width: '100%',\n            fontSize: contents.tableContentFontSize,\n            color: contents.tableContentFontColor,\n          }\"\n          v-if=\"isAuth('yonghu', '查看')\"\n          :data=\"dataList\"\n          v-loading=\"dataListLoading\"\n          @selection-change=\"selectionChangeHandler\"\n        >\n         <el-table-column\n            v-if=\"contents.tableSelection\"\n            type=\"selection\"\n            :header-align=\"contents.tableAlign\"\n            align=\"center\"\n            width=\"50\"\n          >\n          </el-table-column>\n          <el-table-column\n            label=\"索引\"\n            :align=\"contents.tableAlign\"\n            v-if=\"contents.tableIndex\"\n            type=\"index\"\n            width=\"50\"\n          />\n          <el-table-column\n            :sortable=\"contents.tableSortable\"\n            :align=\"contents.tableAlign\"\n            prop=\"yonghuming\"\n            :header-align=\"contents.tableAlign\"\n            label=\"用户名\"\n          >\n            <template slot-scope=\"scope\">\n              {{ scope.row.yonghuming }}\n            </template>\n          </el-table-column>\n          <el-table-column\n            :sortable=\"contents.tableSortable\"\n            :align=\"contents.tableAlign\"\n            prop=\"xingming\"\n            :header-align=\"contents.tableAlign\"\n            label=\"姓名\"\n          >\n            <template slot-scope=\"scope\">\n              {{ scope.row.xingming }}\n            </template>\n          </el-table-column>\n          <el-table-column\n            :sortable=\"contents.tableSortable\"\n            :align=\"contents.tableAlign\"\n            prop=\"xingbie\"\n            :header-align=\"contents.tableAlign\"\n            label=\"性别\"\n          >\n            <template slot-scope=\"scope\">\n              {{ scope.row.xingbie }}\n            </template>\n          </el-table-column>\n          \n          <el-table-column\n            :sortable=\"contents.tableSortable\"\n            :align=\"contents.tableAlign\"\n            prop=\"shouji\"\n            :header-align=\"contents.tableAlign\"\n            label=\"手机\"\n          >\n            <template slot-scope=\"scope\">\n              {{ scope.row.shouji }}\n            </template>\n          </el-table-column>\n          <el-table-column\n            :sortable=\"contents.tableSortable\"\n            :align=\"contents.tableAlign\"\n            prop=\"zhuzhi\"\n            :header-align=\"contents.tableAlign\"\n            label=\"住址\"\n          >\n            <template slot-scope=\"scope\">\n              <!-- {{ scope.row.zhuzhi }} -->\n              {{getYongAddrStr(scope.row)}}\n            </template>\n          </el-table-column>\n           <el-table-column\n            :sortable=\"contents.tableSortable\"\n            :align=\"contents.tableAlign\"\n            prop=\"zhuzhi\"\n            :header-align=\"contents.tableAlign\"\n            label=\"状态\"\n          >\n            <template slot-scope=\"scope\">\n              <!-- {{ scope.row.zhuzhi }} -->\n              <!-- {{getYongAddrStr(scope.row)}} -->\n              正常\n            </template>\n          </el-table-column>\n          <el-table-column\n            width=\"300\"\n            :align=\"contents.tableAlign\"\n            :header-align=\"contents.tableAlign\"\n            label=\"操作\"\n          >\n            <template slot-scope=\"scope\">\n              <el-button\n                v-if=\"\n                  isAuth('yonghu', '查看') &&\n                  contents.tableBtnIcon == 1 &&\n                  contents.tableBtnIconPosition == 1\n                \"\n                type=\"success\"\n                icon=\"el-icon-tickets\"\n                size=\"mini\"\n                @click=\"addOrUpdateHandler(scope.row.id, 'info')\"\n                >{{ contents.tableBtnFont == 1 ? \"详情\" : \"\" }}</el-button\n              >\n              <el-button\n                v-if=\"\n                  isAuth('yonghu', '查看') &&\n                  contents.tableBtnIcon == 1 &&\n                  contents.tableBtnIconPosition == 2\n                \"\n                type=\"success\"\n                size=\"mini\"\n                @click=\"addOrUpdateHandler(scope.row.id, 'info')\"\n                >{{ contents.tableBtnFont == 1 ? \"详情\" : \"\"\n                }}<i class=\"el-icon-tickets el-icon--right\"\n              /></el-button>\n              <el-button\n                v-if=\"isAuth('yonghu', '查看') && contents.tableBtnIcon == 0\"\n                type=\"success\"\n                size=\"mini\"\n                @click=\"addOrUpdateHandler(scope.row.id, 'info')\"\n                >{{ contents.tableBtnFont == 1 ? \"详情\" : \"\" }}</el-button\n              >\n              <el-button\n                v-if=\"\n                  isAuth('yonghu', '修改') &&\n                  contents.tableBtnIcon == 1 &&\n                  contents.tableBtnIconPosition == 1\n                \"\n                type=\"primary\"\n                icon=\"el-icon-edit\"\n                size=\"mini\"\n                @click=\"addOrUpdateHandler(scope.row.id)\"\n                >{{ contents.tableBtnFont == 1 ? \"修改\" : \"\" }}</el-button\n              >\n              <el-button\n                v-if=\"\n                  isAuth('yonghu', '修改') &&\n                  contents.tableBtnIcon == 1 &&\n                  contents.tableBtnIconPosition == 2\n                \"\n                type=\"primary\"\n                size=\"mini\"\n                @click=\"addOrUpdateHandler(scope.row.id)\"\n                >{{ contents.tableBtnFont == 1 ? \"修改\" : \"\"\n                }}<i class=\"el-icon-edit el-icon--right\"\n              /></el-button>\n              <el-button\n                v-if=\"isAuth('yonghu', '修改') && contents.tableBtnIcon == 0\"\n                type=\"primary\"\n                size=\"mini\"\n                @click=\"addOrUpdateHandler(scope.row.id)\"\n                >{{ contents.tableBtnFont == 1 ? \"修改\" : \"\" }}</el-button\n              >\n            </template>\n          </el-table-column>\n        </el-table>\n        <el-pagination\n          clsss=\"pages\"\n          :layout=\"layouts\"\n          @size-change=\"sizeChangeHandle\"\n          @current-change=\"currentChangeHandle\"\n          :current-page=\"pageIndex\"\n          :page-sizes=\"[10, 20, 50, 100]\"\n          :page-size=\"Number(contents.pageEachNum)\"\n          :total=\"totalPage\"\n          :small=\"contents.pageStyle\"\n          class=\"pagination-content\"\n          :background=\"contents.pageBtnBG\"\n          :style=\"{\n            textAlign:\n              contents.pagePosition == 1\n                ? 'left'\n                : contents.pagePosition == 2\n                ? 'center'\n                : 'right',\n          }\"\n        ></el-pagination>\n      </div>\n    </div>\n    <!-- 添加/修改页面  将父组件的search方法传递给子组件-->\n    <add-or-update\n      v-if=\"addOrUpdateFlag\"\n      :parent=\"this\"\n      ref=\"addOrUpdate\"\n    ></add-or-update>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport AddOrUpdate from \"./add-or-update\";\nexport default {\n  data(){\n    return{\n      count: [\n        1300,\n        300,\n        200,\n        300,\n        100,\n        80,\n  