on label=\"发货中\" value=\"0\" clearable></el-option>\r\n            <el-option label=\"已收货\" value=\"1\" clearable></el-option>\r\n            <el-option label=\"退货中\" value=\"2\" clearable></el-option>\r\n            <el-option label=\"已退货\" value=\"3\" clearable></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <el-form-item>\r\n          <el-button @click=\"getDataList()\">查询</el-button>\r\n        </el-form-item>\r\n        <el-row>\r\n          <!-- <el-button type=\"primary\" @click=\"clickAddAreaBtn\">新增</el-button> -->\r\n          <el-button\r\n            type=\"danger\"\r\n            @click=\"deleteHandle()\"\r\n            :disabled=\"dataListSelections.length <= 0\"\r\n            >批量删除</el-button\r\n          >\r\n        </el-row>\r\n      </el-form>\r\n      <div class=\"table-content\">\r\n        <el-table\r\n          :data=\"dataList\"\r\n          border\r\n          v-loading=\"dataListLoading\"\r\n          @selection-change=\"selectionChangeHandle\"\r\n          style=\"width: 100%\"\r\n        >\r\n          <el-table-column\r\n            type=\"selection\"\r\n            header-align=\"center\"\r\n            align=\"center\"\r\n            width=\"50\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column\r\n            type=\"index\"\r\n            header-align=\"center\"\r\n            align=\"center\"\r\n            label=\"序号\"\r\n            width=\"50\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"orderNum\"\r\n            header-align=\"center\"\r\n            align=\"center\"\r\n            label=\"订单号\"\r\n            width=\"150\"\r\n            :show-overflow-tooltip=\"true\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column\r\n            header-align=\"center\"\r\n            align=\"center\"\r\n            label=\"商品名称\"\r\n            width=\"150\"\r\n            prop=\"goodsName\"\r\n      