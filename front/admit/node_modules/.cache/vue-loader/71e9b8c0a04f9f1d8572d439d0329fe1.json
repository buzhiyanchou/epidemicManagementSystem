s.btnAdAllIcon == 0 && contents.tableSelection\"\n              :disabled=\"dataListSelections.length <= 0\"\n              type=\"danger\"\n              @click=\"deleteHandler()\"\n            >{{ contents.btnAdAllFont == 1?'删除':'' }}</el-button>\n\n\n\n\n\n          </el-form-item>\n        </el-row>\n      </el-form>\n      <div class=\"table-content\">\n        <el-table class=\"tables\" :size=\"contents.tableSize\" :show-header=\"contents.tableShowHeader\"\n            :header-row-style=\"headerRowStyle\" :header-cell-style=\"headerCellStyle\"\n            :border=\"contents.tableBorder\"\n            :fit=\"contents.tableFit\"\n            :stripe=\"contents.tableStripe\"\n            :style=\"{width: '100%',fontSize:contents.tableContentFontSize,color:contents.tableContentFontColor}\"\n            v-if=\"isAuth('yiqingshangbao','查看')\"\n            :data=\"dataList\"\n            v-loading=\"dataListLoading\"\n            @selection-change=\"selectionChangeHandler\">\n            <el-table-column  v-if=\"contents.tableSelection\"\n                type=\"selection\"\n                :header-align=\"contents.tableAlign\"\n                align=\"center\"\n                width=\"50\">\n            </el-table-column>\n            <el-table-column label=\"索引\" :align=\"contents.tableAlign\"  v-if=\"contents.tableIndex\" type=\"index\" width=\"50\" />\n                <el-table-column  :sortable=\"contents.tableSortable\" :align=\"contents.tableAlign\" \n                    prop=\"biaoti\"\n                   :header-align=\"contents.tableAlign\"\n\t\t    label=\"标题\">\n\t\t     <template slot-scope=\"scope\">\n                       {{scope.row.biaoti}}\n                     </template>\n                </el-table-column>\n                  <el-table-column :sortable=\"contents.tableSortable\" :align=\"contents.tableAlign\"  prop=\"tupian\"\n                   :header-align=\"contents.tableAlign\"\n                    width=\"200\"\n                    label=\"图片\">\n                    <template slot-scope=\"scope\">\n                      <div v-if=\"scope.row.tupian\">\n                        <img :src=\"$base.url+scope.row.tupian.split(',')[0]\" width=\"100\" height=\"100\">\n                      </div>\n                      <div v-else>无图片</div>\n                    </template>\n                  </el-table-column>\n                <el-table-column  :sortable=\"contents.tableSortable\" :align=\"contents.tableAlign\" \n                    prop=\"shangbaoriqi\"\n                   :header-align=\"contents.tableAlign\"\n\t\t    label=\"上报日期\">\n\t\t     <template slot-scope=\"scope\">\n                       {{scope.row.shangbaoriqi}}\n                     </template>\n                </el-table-column>\n                <el-table-column  :sortable=\"contents.tableSortable\" :align=\"contents.tableAlign\" \n                    prop=\"yonghuming\"\n                   :header-align=\"contents.tableAlign\"\n\t\t    label=\"用户名\">\n\t\t     <template slot-scope=\"scope\">\n                       {{scope.row.yonghuming}}\n                     </template>\n                </el-table-column>\n                <el-table-column  :sortable=\"contents.tableSortable\" :align=\"contents.tableAlign\" \n                    prop=\"shouji\"\n                   :header-align=\"contents.tableAlign\"\n\t\t    label=\"手机\">\n\t\t     <template slot-scope=\"scope\">\n                       {{scope.row.shouji}}\n                     </template>\n                </el-table-column>\n              <el-table-column :sortable=\"contents.tableSortable\" :align=\"contents.tableAlign\" \n                  prop=\"shhf\"\n                 :header-align=\"contents.tableAlign\"\n                  label=\"审核回复\">\n              </el-table-column>\n              <el-table-column :sortable=\"contents.tableSortable\" :align=\"contents.tableAlign\" \n                  prop=\"sfsh\"\n                 :header-align=\"contents.tableAlign\"\n                  lab